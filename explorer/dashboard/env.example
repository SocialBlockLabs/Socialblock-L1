# SocialBlock Explorer Dashboard Environment Configuration

# Backend Service URLs
VITE_RPC_URL=http://localhost:26657
VITE_ARP_AGENT_URL=http://localhost:8080
VITE_INDEXER_URL=http://localhost:3000

# Network Configuration
VITE_NETWORK_ID=socialblock-devnet-1
VITE_CHAIN_ID=socialblock-devnet-1
VITE_DENOM=usblk

# API Configuration
VITE_ARP_AGENT_API_KEY=your_api_key_here
VITE_INDEXER_API_KEY=your_indexer_key_here

# Feature Flags
VITE_ENABLE_AI_AGENTS=true
VITE_ENABLE_REPUTATION_SYSTEM=true
VITE_ENABLE_GOVERNANCE=true
VITE_ENABLE_PLUGINS=true

# UI Configuration
VITE_DEFAULT_THEME=dark
VITE_ENABLE_ANIMATIONS=true
VITE_ENABLE_SOUNDS=false

# Development Settings
VITE_DEBUG_MODE=false
VITE_MOCK_DATA=false
VITE_LOG_LEVEL=info

# Analytics (Optional)
VITE_ANALYTICS_ID=
VITE_ANALYTICS_ENABLED=false

# Social Features
VITE_ENABLE_SOCIAL_FEATURES=true
VITE_ENABLE_FOLLOW_SYSTEM=true
VITE_ENABLE_REPUTATION_SHARING=true
